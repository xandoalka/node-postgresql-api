datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Subscribe {
  NO
  YES
}

model App {
  id          Int     @id @default(autoincrement())
  title       String
  description String?
  developer   String
  logo        String?
  image       String?
  cost        String?
  download    String?
  subscribe   Subscribe @default(NO)
}
